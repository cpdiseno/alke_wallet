<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashFragment4">

    <activity
        android:id="@+id/splashScreenActivity"
        android:name="pena.camila.alkewallet.MainActivity"
        android:label="activity_splash_screen"
        tools:layout="@layout/activity_main" />

    <fragment
        android:id="@+id/loginSignup"
        android:name="pena.camila.alkewallet.fragments.LoginSignup"
        android:label="fragment_login_signup"
        tools:layout="@layout/fragment_login_signup" >
        <action
            android:id="@+id/action_loginSignup_to_signup"
            app:destination="@id/signup" />
        <action
            android:id="@+id/action_loginSignup_to_login_Page"
            app:destination="@id/login_Page" />
    </fragment>


    <fragment
        android:id="@+id/login_Page"
        android:name="pena.camila.alkewallet.fragments.Login_Page"
        android:label="fragment_login_page"
        tools:layout="@layout/fragment_login_page" >
        <action
            android:id="@+id/action_login_Page_to_loginSignup"
            app:destination="@id/loginSignup" />
        <action
            android:id="@+id/action_login_Page_to_signup"
            app:destination="@id/signup" />
        <action
            android:id="@+id/action_login_Page_to_homePage"
            app:destination="@id/homePage" />
    </fragment>
    <fragment
        android:id="@+id/splashFragment4"
        android:name="pena.camila.alkewallet.fragments.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment4_to_loginSignup"
            app:destination="@id/loginSignup" />
    </fragment>
    <fragment
        android:id="@+id/signup"
        android:name="pena.camila.alkewallet.fragments.Signup"
        android:label="fragment_signup"
        tools:layout="@layout/fragment_signup" >
        <action
            android:id="@+id/action_signup_to_emptyCase"
            app:destination="@id/emptyCase" />
        <action
            android:id="@+id/action_signup_to_login_Page"
            app:destination="@id/login_Page" />
    </fragment>
    <fragment
        android:id="@+id/homePage"
        android:name="pena.camila.alkewallet.fragments.homePage"
        android:label="fragment_home_page"
        tools:layout="@layout/fragment_home_page" >
        <action
            android:id="@+id/action_homePage_to_request_money"
            app:destination="@id/request_money" />
        <action
            android:id="@+id/action_homePage_to_send_money"
            app:destination="@id/send_money" />
        <action
            android:id="@+id/action_homePage_to_profile_page"
            app:destination="@id/profile_page" />
        <action
            android:id="@+id/action_homePage_to_send_money2"
            app:destination="@id/send_money" />
        <action
            android:id="@+id/action_homePage_to_request_money2"
            app:destination="@id/request_money" />
    </fragment>
    <fragment
        android:id="@+id/emptyCase"
        android:name="pena.camila.alkewallet.fragments.emptyCase"
        android:label="fragment_empty_case"
        tools:layout="@layout/fragment_empty_case" >
        <action
            android:id="@+id/action_emptyCase_to_send_money"
            app:destination="@id/send_money" />
        <action
            android:id="@+id/action_emptyCase_to_request_money"
            app:destination="@id/request_money" />
    </fragment>
    <fragment
        android:id="@+id/request_money"
        android:name="pena.camila.alkewallet.fragments.request_money"
        android:label="fragment_request_money"
        tools:layout="@layout/fragment_request_money" >
        <action
            android:id="@+id/action_request_money_to_homePage"
            app:destination="@id/homePage" />
    </fragment>
    <fragment
        android:id="@+id/send_money"
        android:name="pena.camila.alkewallet.fragments.send_money"
        android:label="fragment_send_money"
        tools:layout="@layout/fragment_send_money" >
        <action
            android:id="@+id/action_send_money_to_homePage"
            app:destination="@id/homePage" />
        <action
            android:id="@+id/action_send_money_to_emptyCase"
            app:destination="@id/emptyCase" />
    </fragment>
    <fragment
        android:id="@+id/profile_page"
        android:name="pena.camila.alkewallet.fragments.profile_page"
        android:label="fragment_profile_page"
        tools:layout="@layout/fragment_profile_page" />
</navigation>